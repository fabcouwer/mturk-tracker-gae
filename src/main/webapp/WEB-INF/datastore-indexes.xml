<datastore-indexes autoGenerate="true">
  <datastore-index kind="HITinstance" ancestor="false" source="manual">
    <property name="groupId" direction="asc"/>
    <property name="timestamp" direction="desc"/>
  </datastore-index>
  <datastore-index kind="ArrivalCompletions " ancestor="false" source="manual">
    <property name="from" direction="asc"/>
    <property name="to" direction="asc"/>
  </datastore-index>
  <datastore-index kind="HITgroup" ancestor="false">
    <property name="active" direction="asc"/>
    <property name="lastSeen" direction="asc"/>
  </datastore-index>
  <datastore-index kind="HITgroup" ancestor="false" source="manual">
    <property name="requesterId" direction="asc"/>
    <property name="expirationDate" direction="asc"/>
  </datastore-index>
  <datastore-index kind="TopRequester" ancestor="false" source="manual">
    <property name="timestamp" direction="asc"/>
    <property name="reward" direction="desc"/>
  </datastore-index>
</datastore-indexes>
